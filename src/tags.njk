---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - post
    - posts
    - tagList
    - todayLearnedTagList
  addAllPagesToCollections: true
layout: layouts/base.njk
eleventyComputed:
  title: Blog posts tagged “{{ tag }}”
permalink: /posts/tags/{{ tag | slugify }}/
---
<h1 id="Skip">Blog posts tagged “{{ tag }}”</h1>

{% set postslist = collections[tag] | getRegularPosts %}
{% include "partials/postslist.njk" %}

<p>See <a href="{{ '/posts/tags/' | url }}">all blog post tags</a>.</p>