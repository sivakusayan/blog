{
	"name": "blog",
	"version": "1.0.0",
	"description": "A starter repository for a blog web site using the Eleventy static site generator.",
	"scripts": {
		"bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"watch": "npx @11ty/eleventy --watch",
		"serve": "npx @11ty/eleventy --serve",
		"start": "ENVIRONMENT=development npx @11ty/eleventy --serve",
		"build": "ENVIRONMENT=production npx @11ty/eleventy",
		"debug": "DEBUG=* npx @11ty/eleventy",
		"pa11y-ci:sitemap": "pa11y-ci --sitemap http://localhost:8080/sitemap.xml",
		"test:pa11y-ci": "npx start-server-and-test start 8080 pa11y-ci:sitemap"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Sayan Sivakumaran",
		"email": "sivakusayan@gmail.com",
		"url": "https://sayansivakumaran.com/"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"dependencies": {
		"@11ty/eleventy": "3",
		"@11ty/eleventy-navigation": "0.3.5",
		"@11ty/eleventy-plugin-bundle": "1.0.5",
		"@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
		"@staticman/netlify-functions": "^1.7.1",
		"cheerio": "1.0.0-rc.12",
		"cssnano": "7.0.4",
		"dotenv": "16.4.5",
		"global": "^4.4.0",
		"markdown-it": "14.1.0",
		"markdown-it-anchor": "9.0.1",
		"markdown-it-attrs": "^4.2.0",
		"markdown-it-math": "^5.2.0",
		"postcss": "8.4.39",
		"prettier": "^3.3.3",
		"temml": "^0.11.6",
		"uglify-js": "3.18.0"
	},
	"devDependencies": {
		"@11ty/eleventy-plugin-rss": "1.2.0",
		"pa11y-ci": "3.1.0",
		"start-server-and-test": "2.0.4"
	},
	"resolutions": {
		"@11ty/eleventy-plugin-syntaxhighlight/prismjs": "git+https://github.com:sivakusayan/prism.git"
	},
	"packageManager": "yarn@4.1.0"
}
